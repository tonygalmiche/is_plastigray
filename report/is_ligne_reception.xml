<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_ligne_reception_form_view" model="ir.ui.view">        <field name="name">is_ligne_reception_form_view</field>        <field name="model">is.ligne.reception</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="picking_id"    />                        <field name="order_id"    />                        <field name="partner_id"  />                        <field name="product_id"   />                        <field name="ref_fournisseur" />                        <field name="commande_ouverte" />                        <field name="product_uom_qty" />                        <field name="product_uom" />                        <field name="date_planned" />                        <field name="state" />                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_ligne_reception_tree_view" model="ir.ui.view">        <field name="name">is_ligne_reception_tree_view</field>        <field name="model">is.ligne.reception</field>        <field name="arch" type="xml">            <tree create="false">                <field name="picking_id"     widget="many2one_clickable" />                <field name="order_id"     widget="many2one_clickable" />                <field name="partner_id"     widget="many2one_clickable" />                <field name="product_id"     widget="many2one_clickable" />                <field name="ref_fournisseur" />                <field name="commande_ouverte" />                <field name="product_uom_qty" />                <field name="product_uom" />                <field name="date_planned" />                <field name="state" />           </tree>        </field>    </record>    <record id="is_ligne_reception_search_view" model="ir.ui.view">        <field name="name">is_ligne_reception_search_view</field>        <field name="model">is.ligne.reception</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="picking_id"     />                <field name="order_id"     />                <field name="partner_id"     />                <field name="product_id"     />                <field name="ref_fournisseur" />                <field name="commande_ouverte" />                <field name="state" />            </search>        </field>    </record>    <record id="is_ligne_reception_a_traiter_action" model="ir.actions.act_window">        <field name="name">Lignes de réceptions</field>        <field name="res_model">is.ligne.reception</field>        <field name="domain">[('state','not in', ('draft','cancel','waiting','confirmed','done'))]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record>    <record id="is_ligne_reception_action" model="ir.actions.act_window">        <field name="name">Lignes de réceptions</field>        <field name="res_model">is.ligne.reception</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>    </record></data></openerp>