<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_article_sans_fournisseur_tree_view" model="ir.ui.view">        <field name="name">is_article_sans_fournisseur_tree_view</field>        <field name="model">is.article.sans.fournisseur</field>        <field name="arch" type="xml">            <tree create="false">                <field name="is_code" />                <field name="designation" />                <field name="nb" />           </tree>        </field>    </record>    <record id="is_article_sans_fournisseur_search_view" model="ir.ui.view">        <field name="name">is_article_sans_fournisseur_search_view</field>        <field name="model">is.article.sans.fournisseur</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="is_code" />                <field name="designation" />                <field name="nb" />            </search>        </field>    </record>    <record id="is_article_sans_fournisseur_action" model="ir.actions.act_window">        <field name="name">Articles sans fournisseurs</field>        <field name="res_model">is.article.sans.fournisseur</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>