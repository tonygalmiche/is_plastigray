<?xml version="1.0" encoding="utf-8"?><openerp><data>       <record id="is_comparatif_tarif_cial_vente_form_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_cial_vente_form_view</field>        <field name="model">is.comparatif.tarif.cial.vente</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="tarif_cial_id"/>                            <field name="partner_id"/>                            <field name="pricelist_id"/>                            <field name="product_id"/>                            <field name="lot_livraison" />                            <field name="is_category_id" />                            <field name="tarif_cial" />                            <field name="tarif_vente" />                            <field name="delta" />                        </group>                     </group>                </sheet>            </form>        </field>    </record>    <record id="is_comparatif_tarif_cial_vente_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_cial_vente_tree_view</field>        <field name="model">is.comparatif.tarif.cial.vente</field>        <field name="arch" type="xml">            <tree create="false">                <field name="tarif_cial_id"  widget="many2one_clickable" />                <field name="partner_id"     widget="many2one_clickable" />                <field name="pricelist_id"   widget="many2one_clickable" />                <field name="product_id"     widget="many2one_clickable" />                <field name="lot_livraison" />                <field name="is_category_id" />                <field name="tarif_cial" />                <field name="tarif_vente" />                <field name="delta" />           </tree>        </field>    </record>    <record id="is_comparatif_tarif_cial_vente_search_view" model="ir.ui.view">        <field name="name">is_comparatif_tarif_cial_vente_search_view</field>        <field name="model">is.comparatif.tarif.cial.vente</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="is_code" />                <field name="designation" />                <field name="is_category_id" />            </search>        </field>    </record>    <record id="is_comparatif_tarif_cial_vente_action" model="ir.actions.act_window">        <field name="name">Comparatif Tarif commercial et prix de vente</field>        <field name="res_model">is.comparatif.tarif.cial.vente</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form</field>        <field name="help" type="html">            <p class="oe_view_nocontent_create">                <h3>Aucune anomalie trouv√©e</h3>            </p>        </field>    </record></data></openerp>