<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_ligne_livraison_form_view" model="ir.ui.view">        <field name="name">is_ligne_livraison_form_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="date_mouvement" />                            <field name="date_expedition" />                            <field name="date_livraison" />                            <field name="picking_id" />                            <field name="order_id" />                            <field name="client_order_ref" />                            <field name="partner_id"  />                            <field name="product_id" />                            <field name="ref_client" />                            <field name="mold_id" />                            <field name="product_uom_qty"/>                            <field name="product_uom" />                            <field name="nb_uc"/>                            <field name="qt_par_uc" />                        </group>                        <group>                            <field name="user_id" />                            <field name="move_id" />                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_ligne_livraison_search_view" model="ir.ui.view">        <field name="name">is_ligne_livraison_search_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="picking_id"     />                <field name="order_id"     />                <field name="client_order_ref" />                <field name="partner_id"     />                <field name="product_id"     />                <field name="ref_client" />                <field name="mold_id" />            </search>        </field>    </record>    <record id="is_ligne_livraison_tree_view" model="ir.ui.view">        <field name="name">is_ligne_livraison_tree_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <tree create="false">                <field name="date_expedition" />                <field name="date_livraison" />                <field name="picking_id"       widget="many2one_clickable" />                <field name="order_id"         widget="many2one_clickable" />                <field name="client_order_ref" />                <field name="partner_id"       widget="many2one_clickable" />                <field name="product_id"       widget="many2one_clickable" />                <field name="ref_client" />                <field name="mold_id" />                <field name="product_uom_qty"  sum="Total"/>                <field name="product_uom" />                <field name="nb_uc"            sum="Total"/>                <field name="qt_par_uc" />           </tree>        </field>    </record>    <record id="is_ligne_livraison_action" model="ir.actions.act_window">        <field name="name">Lignes de livraisons</field>        <field name="res_model">is.ligne.livraison</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_id" ref="is_ligne_livraison_tree_view"/>        <field name="view_mode">tree,form</field>    </record></data></openerp>