<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_ligne_livraison_form_view" model="ir.ui.view">        <field name="name">is_ligne_livraison_form_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="date_mouvement" />                            <field name="date_expedition" />                            <field name="date_livraison" />                            <field name="client_order_ref" />                            <field name="partner_id"  />                            <field name="product_id" />                            <field name="segment_id" />                            <field name="is_category_id" />                            <field name="is_gestionnaire_id" />                            <field name="ref_client" />                            <field name="mold_id" />                            <field name="product_uom_qty"/>                            <field name="product_uom" />                            <field name="nb_uc"/>                            <field name="qt_par_uc" />                            <field name="price_unit"/>                            <field name="price_subtotal"/>                            <field name="amortissement_moule"/>                            <field name="montant_amt_moule"/>                        </group>                        <group>                            <field name="order_id" />                            <field name="order_line_id" />                            <field name="picking_id" />                            <field name="move_id" />                            <field name="user_id" />                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <record id="is_ligne_livraison_tree_view" model="ir.ui.view">        <field name="name">is_ligne_livraison_tree_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <tree create="false">                <field name="date_expedition" />                <field name="date_livraison" />                <field name="picking_id"        widget="many2one_clickable" />                <field name="order_id"          widget="many2one_clickable" />                <field name="client_order_ref" />                <field name="partner_id"        widget="many2one_clickable" />                <field name="product_id"        widget="many2one_clickable" />                <field name="ref_client" />                <field name="mold_id" />                <field name="product_uom_qty"   sum="Total"/>                <field name="product_uom" />                <field name="nb_uc"             sum="Total"/>                <field name="qt_par_uc" />           </tree>        </field>    </record>    <record id="is_ligne_livraison_search_view" model="ir.ui.view">        <field name="name">is_ligne_livraison_search_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="picking_id"     />                <field name="order_id"     />                <field name="client_order_ref" />                <field name="partner_id"     />                <field name="product_id"     />                <field name="ref_client" />                <field name="mold_id" />                <field name="segment_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <group expand="0" string="Regrouper par...">                    <filter string="Client"       context="{'group_by':'partner_id'}"/>                    <filter string="Moule"        context="{'group_by':'product_id'}"/>                    <filter string="Article"      context="{'group_by':'mold_id'}"/>                    <filter string="Segment"      context="{'group_by':'segment_id'}"/>                    <filter string="Catégorie"    context="{'group_by':'is_category_id'}"/>                    <filter string="Gestionnaire" context="{'group_by':'is_gestionnaire_id'}"/>                </group>            </search>        </field>    </record>    <record id="is_ligne_livraison_action" model="ir.actions.act_window">        <field name="name">Lignes de livraisons</field>        <field name="res_model">is.ligne.livraison</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_id" ref="is_ligne_livraison_tree_view"/>        <field name="view_mode">tree,form</field>    </record>    <!-- Indicateur CA sur livraisons -->    <record id="is_indicateur_ca_livraison_tree_view" model="ir.ui.view">        <field name="name">is_indicateur_ca_livraison_tree_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <tree create="false">                <field name="date_expedition" />                <field name="picking_id"        widget="many2one_clickable" />                <field name="partner_id"        widget="many2one_clickable" />                <field name="product_id"        widget="many2one_clickable" />                <field name="mold_id" />                <field name="segment_id" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="product_uom_qty"   sum="Total"/>                <field name="product_uom" />                <field name="price_subtotal"    sum="Total"/>                <field name="montant_amt_moule" sum="Total"/>                <field name="montant_matiere"   sum="Total"/>           </tree>        </field>    </record>    <record id="is_indicateur_ca_livraison_graph_view" model="ir.ui.view">        <field name="name">is_indicateur_ca_livraison_graph_view</field>        <field name="model">is.ligne.livraison</field>        <field name="arch" type="xml">            <graph type="pivot">                <field name="partner_id"        type="row"/>                <field name="date_expedition"   type="col"/>                <field name="price_subtotal"    type="measure"/>                <field name="montant_amt_moule" type="measure"/>                <field name="montant_matiere"   type="measure"/>            </graph>        </field>    </record>    <!-- Action en précisant les vues graph et tree -->    <record id="is_indicateur_ca_livraison_action" model="ir.actions.act_window">        <field name="name">CA sur livraisons</field>        <field name="res_model">is.ligne.livraison</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">graph,tree,form</field>    </record>    <record id="is_indicateur_ca_livraison_action_graph" model="ir.actions.act_window.view" >        <field name="sequence" eval="1"/>        <field name="view_mode">graph</field>        <field name="view_id" ref="is_indicateur_ca_livraison_graph_view"/>        <field name="act_window_id" ref="is_indicateur_ca_livraison_action"/>    </record>    <record id="is_indicateur_ca_livraison_action_tree" model="ir.actions.act_window.view" >        <field name="sequence" eval="2"/>        <field name="view_mode">tree</field>        <field name="view_id" ref="is_indicateur_ca_livraison_tree_view"/>        <field name="act_window_id" ref="is_indicateur_ca_livraison_action"/>    </record></data></openerp>