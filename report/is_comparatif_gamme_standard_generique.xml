<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_comparatif_gamme_standard_generique_tree_view" model="ir.ui.view">        <field name="name">is_comparatif_gamme_standard_generique_tree_view</field>        <field name="model">is.comparatif.gamme.standard.generique</field>        <field name="arch" type="xml">            <tree>                <field name="product_id" />                <field name="standard_routing_id" />                <field name="standard_sequence" />                <field name="standard_name" />                <field name="standard_workcenter_id" />                <field name="standard_nb_secondes" />           </tree>        </field>    </record>    <record id="is_comparatif_gamme_standard_generique_search_view" model="ir.ui.view">        <field name="name">is_comparatif_gamme_standard_generique_search_view</field>        <field name="model">is.comparatif.gamme.standard.generique</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="product_id" />            </search>        </field>    </record>    <record id="is_comparatif_gamme_standard_generique_action" model="ir.actions.act_window">        <field name="name">Comparatif gamme standard générique</field>        <field name="res_model">is.comparatif.gamme.standard.generique</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>