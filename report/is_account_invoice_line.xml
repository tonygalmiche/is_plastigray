<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_account_invoice_line_form_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_form_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <group>                            <field name="partner_id"/>                            <field name="date_invoice"/>                            <field name="invoice_id"/>                            <field name="internal_number"/>                            <field name="date_due"/>                            <field name="origin"/>                            <field name="supplier_invoice_number"/>                            <field name="state"/>                            <field name="type"/>                            <field name="product_id"/>                            <field name="description"/>                            <field name="is_ref_client"/>                            <field name="quantity"/>                            <field name="uos_id"/>                            <field name="price_unit"/>                        </group>                        <group>                            <field name="move_id"/>                            <field name="picking_id"/>                            <field name="purchase_order_id"/>                        </group>                    </group>                </sheet>            </form>        </field>    </record>    <!-- Lignes de factures client -->    <record id="is_account_invoice_line_customer_tree_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_tree_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <tree create="false">                <field name="partner_id"           widget="many2one_clickable"/>                <field name="invoice_id"           widget="many2one_clickable"/>                <field name="internal_number"/>                <field name="date_invoice"/>                <field name="date_due"/>                <field name="origin"/>                <field name="product_id"           widget="many2one_clickable"/>                <field name="description"/>                <field name="is_ref_client"/>                <field name="uos_id"/>                <field name="quantity" sum="Total"/>                <field name="price_unit"/>                <field name="total"    sum="Total"/>                <field name="state"/>                <field name="picking_id"           widget="many2one_clickable"/>           </tree>        </field>    </record>    <!-- Lignes de factures fournisseur -->    <record id="is_account_invoice_line_supplier_tree_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_tree_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <tree create="false">                <field name="partner_id"           widget="many2one_clickable"/>                <field name="invoice_id"           widget="many2one_clickable"/>                <field name="internal_number"/>                <field name="date_invoice"/>                <field name="date_due"/>                <field name="product_id"           widget="many2one_clickable"/>                <field name="description"/>                <field name="uos_id"/>                <field name="quantity" sum="Total"/>                <field name="price_unit"/>                <field name="total"    sum="Total"/>                <field name="state"/>                <field name="picking_id"           widget="many2one_clickable"/>                <field name="purchase_order_id"    widget="many2one_clickable"/>           </tree>        </field>    </record>    <record id="is_account_invoice_line_tree_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_tree_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <tree create="false">                <field name="partner_id"           widget="many2one_clickable"/>                <field name="invoice_id"           widget="many2one_clickable"/>                <field name="internal_number"/>                <field name="date_invoice"/>                <field name="date_due"/>                <field name="origin"/>                <field name="product_id"           widget="many2one_clickable"/>                <field name="description"/>                <field name="is_ref_client"/>                <field name="uos_id"/>                <field name="quantity"/>                <field name="price_unit"/>                <field name="total"/>                <field name="state"/>                <field name="picking_id"           widget="many2one_clickable"/>                <field name="purchase_order_id"    widget="many2one_clickable"/>           </tree>        </field>    </record>    <record id="is_account_invoice_line_search_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_search_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="partner_id"/>                <field name="internal_number"/>                <field name="origin"/>                <field name="supplier_invoice_number"/>                <field name="state"/>                <field name="type"/>                <field name="product_id"/>                <field name="description"/>                <field name="is_ref_client"/>                <group expand="0" string="Regrouper par...">                    <filter string="Client/Fournisseur" context="{'group_by':'partner_id'}"/>                    <filter string="Article"            context="{'group_by':'product_id'}"/>                    <filter string="Date facture"       context="{'group_by':'date_invoice'}"/>                </group>            </search>        </field>    </record>    <record id="is_account_invoice_line_graph_view" model="ir.ui.view">        <field name="name">is_account_invoice_line_graph_view</field>        <field name="model">is.account.invoice.line</field>        <field name="arch" type="xml">            <graph type="pivot">                <field name="partner_id"/>                <field name="date_invoice"       type="col"/>                <field name="total"              type="measure"/>            </graph>        </field>    </record>    <record id="is_ligne_facture_fournisseur_action" model="ir.actions.act_window">        <field name="name">Lignes factures fournisseur</field>        <field name="res_model">is.account.invoice.line</field>        <field name="view_id" ref="is_account_invoice_line_supplier_tree_view"/>        <field name="domain">[('type','=', 'in_invoice')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,graph</field>    </record>    <record id="is_ligne_facture_client_action" model="ir.actions.act_window">        <field name="name">Lignes factures client</field>        <field name="res_model">is.account.invoice.line</field>        <field name="view_id" ref="is_account_invoice_line_customer_tree_view"/>        <field name="domain">[('type','=', 'out_invoice')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree,form,graph</field>    </record></data></openerp>