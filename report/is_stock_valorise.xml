<?xml version="1.0" encoding="utf-8"?><openerp><data>    <record id="is_stock_valorise_tree_view" model="ir.ui.view">        <field name="name">is_stock_valorise_tree_view</field>        <field name="model">is.stock.valorise</field>        <field name="arch" type="xml">            <tree create="false">                <field name="is_code" />                <field name="designation" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />                <field name="stock_a" />                <field name="stock_q" />                <field name="stock" />                <field name="cout_act_matiere" />                <field name="cout_act_machine" />                <field name="cout_act_mo" />                <field name="cout_act_st" />                <field name="cout_act_total" />                <field name="stock_valorise" />           </tree>        </field>    </record>    <record id="is_stock_valorise_search_view" model="ir.ui.view">        <field name="name">is_stock_valorise_search_view</field>        <field name="model">is.stock.valorise</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="is_code" />                <field name="designation" />                <field name="is_category_id" />                <field name="is_gestionnaire_id" />            </search>        </field>    </record>    <record id="is_stock_valorise_action" model="ir.actions.act_window">        <field name="name">Liste des stocks valoris√©s</field>        <field name="res_model">is.stock.valorise</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>