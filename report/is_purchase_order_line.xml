<?xml version="1.0" encoding="utf-8"?><openerp><data>   <record id="is_purchase_order_line_form_view" model="ir.ui.view">        <field name="name">is_purchase_order_line_form_view</field>        <field name="model">is.purchase.order.line</field>        <field name="arch" type="xml">            <form>                <sheet>                    <group>                        <field name="order_id" />                        <field name="partner_id" />                        <field name="date_order" />                        <field name="is_num_da" />                        <field name="is_document" />                        <field name="is_demandeur_id" />                        <field name="minimum_planned_date" />                        <field name="is_date_confirmation" />                        <field name="is_commentaire" />                        <field name="product_id" />                        <field name="is_ref_fournisseur" />                        <field name="date_planned" />                        <field name="product_qty" />                        <field name="product_uom" />                        <field name="price_unit" />                        <field name="is_justification" />                     </group>                </sheet>            </form>        </field>    </record>    <record id="is_purchase_order_line_search_view" model="ir.ui.view">        <field name="name">is_purchase_order_line_search_view</field>        <field name="model">is.purchase.order.line</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="order_id" />                <field name="partner_id" />                <field name="product_id" />                <field name="is_ref_fournisseur" />                <field name="is_num_da" />                <field name="is_document" />                <field name="is_demandeur_id" />            </search>        </field>    </record>    <record id="is_purchase_order_line_tree_view" model="ir.ui.view">        <field name="name">is_purchase_order_line_tree_view</field>        <field name="model">is.purchase.order.line</field>        <field name="arch" type="xml">            <tree create="false">                <field name="order_id"           widget="many2one_clickable"  />                <field name="partner_id"         widget="many2one_clickable"  />                <field name="date_order" />                <field name="is_num_da" />                <field name="is_document" />                <field name="is_demandeur_id" />                <field name="minimum_planned_date" />                <field name="is_date_confirmation" />                <field name="is_commentaire" />                <field name="product_id"         widget="many2one_clickable"  />                <field name="is_ref_fournisseur" />                <field name="date_planned" />                <field name="product_qty" />                <field name="product_uom" />                <field name="price_unit" />           </tree>        </field>    </record>    <record id="is_purchase_order_line_action" model="ir.actions.act_window">        <field name="name">Lignes de commandes</field>        <field name="res_model">is.purchase.order.line</field>        <field name="domain">[]</field>        <field name="view_type">form</field>        <field name="view_id" ref="is_purchase_order_line_tree_view"/>        <field name="view_mode">tree,form</field>    </record></data></openerp>