<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_cas_emplois_tree_view" model="ir.ui.view">        <field name="name">is_cas_emplois_tree_view</field>        <field name="model">is.cas.emplois</field>        <field name="arch" type="xml">            <tree create="false"  editable="bottom">                <field name="bom_id"            readonly="1"/>                <field name="is_sous_traitance" readonly="1"/>                <field name="is_negoce"         readonly="1"/>                <field name="is_inactive"       readonly="1"/>                <field name="sequence"          readonly="1"/>                <field name="product_id"        readonly="1"/>                <field name="type"              readonly="1"/>                <field name="is_article_fourni" readonly="1"/>                <field name="product_qty"       readonly="1"/>                <field name="product_uom"       readonly="1"/>                <field name="date_start"        readonly="1"/>                <field name="date_stop"         readonly="1"/>           </tree>        </field>    </record>    <record id="is_cas_emplois_search_view" model="ir.ui.view">        <field name="name">is_cas_emplois_search_view</field>        <field name="model">is.cas.emplois</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="bom_id"            readonly="1"/>                <field name="product_id"        readonly="1"/>                <field name="is_sous_traitance" readonly="1"/>                <field name="is_negoce"         readonly="1"/>                <field name="is_inactive"       readonly="1"/>            </search>        </field>    </record>    <record id="is_cas_emplois_action" model="ir.actions.act_window">        <field name="name">Cas d'emplois</field>        <field name="res_model">is.cas.emplois</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>