<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_cas_emplois_tree_view" model="ir.ui.view">        <field name="name">is_cas_emplois_tree_view</field>        <field name="model">is.cas.emplois</field>        <field name="arch" type="xml">            <tree create="false">                <field name="bom_id"               widget="many2one_clickable"/>                <field name="is_mold_id"/>                <field name="is_dossierf_id"/>                <field name="is_sous_traitance"/>                <field name="is_negoce"/>                <field name="is_inactive"/>                <field name="sequence"/>                <field name="product_id"           widget="many2one_clickable"/>                <field name="type"/>                <field name="is_article_fourni"/>                <field name="product_qty"/>                <field name="product_uom"/>           </tree>        </field>    </record>    <record id="is_cas_emplois_search_view" model="ir.ui.view">        <field name="name">is_cas_emplois_search_view</field>        <field name="model">is.cas.emplois</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="bom_id"/>                <field name="is_mold_id"/>                <field name="is_dossierf_id"/>                <field name="product_id"/>                <field name="is_sous_traitance"/>                <field name="is_negoce"/>                <field name="is_inactive"/>            </search>        </field>    </record>    <record id="is_cas_emplois_action" model="ir.actions.act_window">        <field name="name">Cas d'emplois</field>        <field name="res_model">is.cas.emplois</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>