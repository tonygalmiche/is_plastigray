<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_pricelist_item_search_view" model="ir.ui.view">        <field name="name">is_pricelist_item_search_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="pricelist_name" />                <field name="pricelist_type" />                <field name="price_version_id" />                <field name="product_id" />                <field name="ref_client" />                <field name="ref_fournisseur" />                <field name="mold_id" />            </search>        </field>    </record>    <record id="is_pricelist_item_purchase_tree_view" model="ir.ui.view">        <field name="name">is_pricelist_item_tree_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <tree create="false">                <field name="pricelist_name"/>                <button string="Liste des prix" name= "action_liste_items" type="object" icon="gtk-justify-fill"/>                <field name="price_version_id"     widget="many2one_clickable"/>                <field name="version_date_start"/>                <field name="version_date_end"/>                <field name="product_id"           widget="many2one_clickable"/>                <field name="ref_fournisseur"/>                <field name="mold_id" />                <field name="sequence"/>                <field name="min_quantity"            digits="(14, 4)" />                <field name="product_po_uom_id"/>                <field name="price_surcharge"         digits="(14, 4)" />                <field name="item_date_start"/>                <field name="item_date_end"/>           </tree>        </field>    </record>    <record id="is_pricelist_item_purchase_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="view_id" ref="is_pricelist_item_purchase_tree_view"/>        <field name="domain">[('pricelist_type', '=', 'purchase')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record>    <record id="is_pricelist_item_sale_tree_view" model="ir.ui.view">        <field name="name">is_pricelist_item_tree_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <tree create="false">                <field name="pricelist_name" />                <button string="Liste des prix" name= "action_liste_items" type="object" icon="gtk-justify-fill"/>                <field name="price_version_id"     widget="many2one_clickable"/>                <field name="version_date_start"/>                <field name="version_date_end" />                <field name="product_id"           widget="many2one_clickable"/>                <field name="ref_client"/>                <field name="mold_id"/>                <field name="sequence"/>                <field name="min_quantity"         digits="(14, 4)" />                <field name="product_uom_id"/>                <field name="price_surcharge"      digits="(14, 4)" />                <field name="item_date_start"/>                <field name="item_date_end"/>           </tree>        </field>    </record>    <record id="is_pricelist_item_sale_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="view_id" ref="is_pricelist_item_sale_tree_view"/>        <field name="domain">[('pricelist_type', '=', 'sale')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>