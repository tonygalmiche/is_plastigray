<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_pricelist_item_tree_view" model="ir.ui.view">        <field name="name">is_pricelist_item_tree_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <tree create="false"  editable="bottom">                <field name="pricelist_name"     readonly="1"/>                <button string="Liste des prix" name= "action_liste_items" type="object" icon="gtk-justify-fill"/>                <field name="price_version_id"   readonly="1" />                <field name="version_date_start" readonly="1" />                <field name="version_date_end"   readonly="1" />                <field name="product_id"         readonly="1" />                <field name="sequence"           readonly="1" />                <field name="product_po_uom_id"  readonly="1" />                <field name="min_quantity"       readonly="1" digits="(14, 4)" />                <field name="price_surcharge"    readonly="1" />                <field name="item_date_start"    readonly="1" />                <field name="item_date_end"      readonly="1" />           </tree>        </field>    </record>    <record id="is_pricelist_item_search_view" model="ir.ui.view">        <field name="name">is_pricelist_item_search_view</field>        <field name="model">is.pricelist.item</field>        <field name="arch" type="xml">            <search string="Recherche">                <field name="pricelist_name" />                <field name="pricelist_type" />                <field name="price_version_id" />                <field name="product_id" />            </search>        </field>    </record>    <record id="is_pricelist_item_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="domain">[('pricelist_type', '=', 'purchase')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record>    <record id="is_pricelist_item_purchase_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="domain">[('pricelist_type', '=', 'purchase')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record>    <record id="is_pricelist_item_sale_action" model="ir.actions.act_window">        <field name="name">Liste de prix</field>        <field name="res_model">is.pricelist.item</field>        <field name="domain">[('pricelist_type', '=', 'sale')]</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record></data></openerp>