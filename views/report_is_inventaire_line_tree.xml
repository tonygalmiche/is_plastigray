<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
<template id="report_is_inventaire_line_tree">
    <t t-call="report.html_container">
    <t t-call="report.internal_layout">
        <div class="page">
            <div class="oe_structure"/>
            <h2>Lignes d'inventaire</h2>
            <table class="table table-condensed">
                <thead>
                    <tr>
                        <th><strong>Inventaire</strong></th>
                        <th><strong>Feuille</strong></th>
                        <th><strong>Séquence</strong></th>
                        <th><strong>Article</strong></th>
                        <th><strong>Encours</strong></th>
                        <th><strong>Composant</strong></th>
                        <th><strong>UC</strong></th>
                        <th><strong>US par UC</strong></th>
                        <th><strong>Emplacement</strong></th>
                        <th><strong>Qt US saisie</strong></th>
                        <th><strong>Qt UC saisie</strong></th>
                        <th><strong>Qt US</strong></th>
                        <th><strong>Lieu</strong></th>
                        <th><strong>Lot</strong></th>
                        <th><strong>État</strong></th>
                    </tr>
                </thead>
                <tbody>
                    <tr t-foreach="docs" t-as="o">
                        <td><span t-field="o.inventaire_id.name"/> </td>
                        <td><span t-field="o.feuille_id.name"/> </td>
                        <td><span t-field="o.sequence"/> </td>
                        <td><span t-field="o.product_id.name"/> </td>
                        <td><span t-field="o.encours"/> </td>
                        <td><span t-field="o.composant_encours"/> </td>
                        <td><span t-field="o.uc"/> </td>
                        <td><span t-field="o.uc_us"/> </td>
                        <td><span t-field="o.location_id.name"/> </td>
                        <td><span t-field="o.qt_us"/> </td>
                        <td><span t-field="o.qt_uc"/> </td>
                        <td><span t-field="o.qt_us_calc"/> </td>
                        <td><span t-field="o.lieu"/> </td>
                        <td><span t-field="o.lot_id.name"/> </td>
                        <td><span t-field="o.state"/> </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </t>
    </t>
</template>

</data>
</openerp>
